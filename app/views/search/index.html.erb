<div class="search-page">
  <% if @posts.exists? %>
  <br>
    <h3>Top Posts</h3>
  <% else %>
  <br>
    <h3>No results found.</h3>
  <% end %>
<div class="user-images">
    <% @posts.each do |post|%>
      <div class="wrapper">
        <%=image_tag post.image %>
      </div>
    <% end %>
  </div>
</div>
