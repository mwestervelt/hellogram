<div class="search-page">
  <% if @posts.exists? %>
  <br>
    <h3>Top Posts</h3>
  <% else %>
  <br>
    <h3>No results found.</h3>
  <% end %>
<div class="user-images">
    <% @posts.each do |post|%>
      <div class="trim">
        <%=link_to post_path(post) do %>
          <%=image_tag post.image , class: 'main-image' %>
        <% end %>
      </div>
    <% end %>
  </div>
</div>
